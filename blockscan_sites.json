{
    "eth": {
      "url": "https://etherscan.io/address/{address}",
      "xpath": "//div[contains(@class, 'text-cap') and contains(text(), 'ETH Balance')]/following-sibling::div//div//text()"
    },
    "polygon": {
      "url": "https://polygonscan.com/address/{address}",
      "xpath": "//span[contains(text(), 'POL')]/text()"
    },
    "bsc": {
      "url": "https://bscscan.com/address/{address}",
      "xpath": "//div[contains(@class, 'text-cap') and contains(text(), 'BNB Balance')]/following-sibling::div//div//text()"
    },
    "avax": {
      "url": "https://snowtrace.io/address/{address}",
      "xpath": "//div[@class='grid grid-cols-12 items-center']//span[contains(text(), 'AVAX')]/text()"
    },
    "op": {
      "url": "https://optimistic.etherscan.io/address/{address}",
      "xpath": "//div[contains(@class, 'text-cap') and contains(text(), 'ETH Balance')]/following-sibling::div//div//text()"
    },
    "vana": {
      "url": "https://vanascan.io/address/{address}",
      "xpath": "//span//p[contains(text(), 'VANA')]/text()"
    },
    "linea": {
      "url": "https://lineascan.build/address/{address}",
